<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style x:Key="RecycleBinIconStyle" TargetType="{x:Type Path}">
        <Setter Property="Stroke" Value="#ff8393a3" />
        <Setter Property="StrokeThickness" Value="2" />
        <Setter Property="Stretch" Value="Uniform" />
        <Setter Property="Margin" Value="7" />
    </Style>

    <Path x:Key="RecycleBinIcon"
          Style="{StaticResource RecycleBinIconStyle}">
        <Path.Data>
            M 1 1.5 H 15
            M 7 1.5 V 0 H 9 V 1.5
        
            M 3 3 L 4 16 H 12 L 13 3
            
            M 8 5 V 13
            M 5.5 5 L 6 13
            M 10.5 5 L 10 13
        </Path.Data>
    </Path>

</ResourceDictionary>