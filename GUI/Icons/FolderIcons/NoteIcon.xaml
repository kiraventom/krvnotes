<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Path x:Key="NoteIcon"
          x:Shared="False">
        <Path.Data>
            <!--note-->
            M 1 0 V 8 H 7 V 2 h -2 v -2 Z

            <!--angle-->
            M 5 0 l 2 2

            <!--lines-->
            M 2 2 H 4
            M 2 3.25 H 6
            M 2 4.5 H 6
            M 2 5.75 H 6
        </Path.Data>
    </Path>
</ResourceDictionary>