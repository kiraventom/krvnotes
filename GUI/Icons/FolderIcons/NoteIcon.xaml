<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Path x:Key="NoteIcon">
        <Path.Data>
            <!--note-->
            M 0 0 V 8 H 8 V 2 h -2 v -2 Z

            <!--angle-->
            M 6 0 l 2 2

            <!--lines-->
            M 1.25 1.75 H 5
            M 1.25 3 H 6.75
            M 1.25 4.25 H 6.75
            M 1.25 5.5 H 6.75
        </Path.Data>
    </Path>
</ResourceDictionary>