<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Path x:Key="ArchiveIcon">
        <Path.Data>
            <!--box-->
            M 0 2 H 8 V 8 H 0 Z
            M 0 2 l 1.5 -2 H 6.5 l 1.5 2

            <!--bookmarks-->
            M 0.9 0.7 h 2
            M 7.2 1.2 h -2

            <!--arrow-->
            M 4 2 v 2.3
            M 4 4.3 l 0.9 -0.9
            M 4 4.3 l -0.9 -0.9
        </Path.Data>
    </Path>
</ResourceDictionary>