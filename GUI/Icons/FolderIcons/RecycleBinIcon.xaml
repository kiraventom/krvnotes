<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Path x:Key="RecycleBinIcon">
        <Path.Data>
            <!--cover-->
            M 0 1.5 H 16
            M 7 1.5 v -1.5 h 2 v 1.5
        
            <!--bin-->
            M 2 3 l 1 13 h 10 l 1 -13
            M 1 3 h 14
            
            <!--lines-->
            M 8 5.5 v 8
            M 5 5.5 l 0.5 8
            M 11 5.5 l -0.5 8
        </Path.Data>
    </Path>

</ResourceDictionary>