<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style x:Key="NoteIconStyle" TargetType="{x:Type Path}">
        <Setter Property="Stroke" Value="#ff8393a3" />
        <Setter Property="StrokeThickness" Value="2" />
        <Setter Property="Stretch" Value="Uniform" />
        <Setter Property="Margin" Value="10" />
    </Style>

    <Path x:Key="NoteIcon"
          Style="{StaticResource NoteIconStyle}">
        <Path.Data>
            M 0 0 V 8 H 8 V 2 h -2 v -2 Z
            M 6 0 l 2 2
            M 1.5 2 H 5
            M 1.5 3.5 H 6.5
            M 1.5 5 H 6.5
        </Path.Data>
    </Path>
</ResourceDictionary>