<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:cts="clr-namespace:GUI.ControlTemplates">
    <Style x:Key="NotesPanelStyle" TargetType="{x:Type WrapPanel}">
        <Setter Property="Background" Value="OldLace" />
        <Style.Resources>
            <Style TargetType="{x:Type cts:Note}" BasedOn="{StaticResource NoteStyle}"/>
        </Style.Resources>
    </Style>
</ResourceDictionary>